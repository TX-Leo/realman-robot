<?xml version="1.0"?>
<launch>
  <arg name="r_arm_ip" default="192.168.1.19" />
  <arg name="l_arm_ip" default="192.168.1.18" />

  <!-- RM-65 Control -->
  <node ns="r_arm" pkg="arm_control" name="arm_control" type="arm_control" output="screen"> <!--launch-prefix="xterm−e"-->
  </node>
  <node ns="l_arm" pkg="arm_control" name="arm_control" type="arm_control" output="screen"> <!--launch-prefix="xterm−e"-->
  </node>
  
  <!-- RM-65 Driver -->
  <node ns="r_arm" name="arm_driver" pkg="arm_driver" type="arm_driver" respawn="false" output="screen">
    <param name="arm_ip" value="$(arg r_arm_ip)"/>
    <rosparam param="arm_joints" >[r_joint1, r_joint2, r_joint3, r_joint4, r_joint5, r_joint6]</rosparam>
  </node>

  <node ns="l_arm" name="arm_driver" pkg="arm_driver" type="arm_driver" respawn="false" output="screen">
    <param name="arm_ip" value="$(arg l_arm_ip)"/>
    <rosparam param="arm_joints" >[l_joint1, l_joint2, l_joint3, l_joint4, l_joint5, l_joint6]</rosparam>
  </node>

  <!-- Moveit -->
  <include file="$(find dual_arm_65_moveit_config)/launch/demo_realrobot.launch">
  </include>

</launch>
